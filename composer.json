{
    "name": "popouille/leaderboard",
    "description": "Points and Ranks for Eloquent models.",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Leaderboard",
        "Laravel Leaderboard"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "illuminate/database": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.4",
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Popouille\\Leaderboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Popouille\\Tests\\Leaderboard\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Popouille\\Leaderboard\\LeaderboardServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
